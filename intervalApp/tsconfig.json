{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "incremental": true,
    "baseUrl": ".",
    "allowJs": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "noFallthroughCasesInSwitch": true,
    "paths": {
      "@Interval/*": [
        "./src/*"
      ]
    },
    "strictNullChecks": true,
    "sourceMap": true
  },
  "include": [
    "src",
    "intervalApp/next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "intervalApp/next.config.js",
    "postcss.config.js"
  ],
  "exclude": [
    "node_modules"
  ],
}
