{
  "name": "@interval/app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "sass src/styles/scss/global.scss src/styles/global.css && NODE_ENV=development next dev",
    "build": "sass src/styles/scss/global.scss src/styles/global.css && esbuild --outfile=public/sw.js --bundle src/serviceWorker/swRaw.js && NODE_ENV=production next build",
    "start": "NODE_ENV=production next start",
    "fixEslint": "./node_modules/.bin/eslint --fix . && next lint",
    "eslint": "../node_modules/.bin/next lint",
    "compileScss": "../node_modules/.bin/sass src/styles/scss/global.scss src/styles/global.css",
    "packages:force-node-modules-sync": "sudo find node_modules/.pnpm -name 'file+*' -type d -prune -print -exec rm -rf '{}' + && pnpm install",
    "remove:node-modules": "sudo find . -name 'node_modules' -type d -prune -print -exec rm -rf '{}' +"
  },
  "dependencies": {
    "@formatjs/intl-localematcher": "^0.2.32",
    "eslint-plugin-no-relative-import-paths": "1.5.3",
    "@types/lodash": "^4.14.194",
    "@types/node": "^18.16.0",
    "@types/react": "18.0.33",
    "@types/react-dom": "18.0.11",
    "@xstate/cli": "^0.5.2",
    "@xstate/react": "^3.2.2",
    "concurrently": "^8.2.0",
    "esbuild": "^0.18.16",
    "eslint-config-next": "^14.0.3",
    "joi": "17.9.2",
    "jose": "^4.14.6",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "negotiator": "^0.6.3",
    "next": "^14.0.3",
    "next-with-workbox": "^3.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-textarea-autosize": "^8.5.3",
    "rxjs": "^7.8.1",
    "uuid": "^9.0.0",
    "watch": "^1.0.2",
    "webpack": "^5.88.2",
    "webpack-manifest-plugin": "^5.0.0",
    "xstate": "^4.38.2"
  },
  "devDependencies": {
    "@interval/eslint-config-base": "file:../packages/eslint-config/base",
    "@interval/eslint-config-browser-env": "file:../packages/eslint-config/browser-env",
    "@interval/eslint-config-react": "file:../packages/eslint-config/react",
    "@types/negotiator": "^0.6.1",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "eslint": "8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "3.5.2",
    "eslint-plugin-compat": "4.0.2",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-functional": "6.0.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-interval-app-styleguide": "file:../packages/eslint-plugin-interval-app-styleguide",
    "eslint-plugin-no-unsanitized": "4.0.1",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-redux": "^4.1.0",
    "eslint-plugin-security": "^1.7.1",
    "prettier": "3.1.0",
    "sass": "^1.62.1",
    "typescript": "^5.0.4"
  }
}
